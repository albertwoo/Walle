<!DOCTYPE html>
<html lang="en" style="height: 100%; overflow: hidden; overscroll-behavior: none;">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link href="_content/Microsoft.Fast.Components.FluentUI/css/reboot.css" rel="stylesheet" />
    <link rel="stylesheet" href="Walle.styles.css" />
    <link rel="icon" type="image/png" href="favicon.png" />
    <HeadOutlet />
</head>

<body style="height: 100%; overflow: hidden;">
    <main style="height: 100%; overflow: hidden;">
        <Router AppAssembly="@typeof(Program).Assembly">
            <Found Context="routeData">
                <RouteView RouteData="@routeData" />
                <FocusOnNavigate RouteData="@routeData" Selector="h1" />
            </Found>
        </Router>
        <FluentDialogProvider @rendermode="RenderMode.InteractiveServer" />
        <FluentToastContainer @rendermode="RenderMode.InteractiveServer" />
        <FluentMessageBarContainer @rendermode="RenderMode.InteractiveServer" />
    </main>
    <script src="_framework/blazor.web.js"></script>
</body>

</html>
